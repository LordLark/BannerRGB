<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id = "@+id/main_activity_view"
    android:tag = "large"
    tools:context=".MainActivity">

    <SeekBar
        android:id="@+id/redBar"
        android:layout_width="421dp"
        android:layout_height="54dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="79dp"
        android:layout_marginTop="191dp"
        android:layout_marginEnd="524dp"
        android:layout_marginBottom="386dp" />

    <SeekBar
        android:id="@+id/greenBar"
        android:layout_width="422dp"
        android:layout_height="59dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="81dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="521dp"
        android:layout_marginBottom="322dp" />

    <SeekBar
        android:id="@+id/blueBar"
        android:layout_width="428dp"
        android:layout_height="76dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="81dp"
        android:layout_marginTop="307dp"
        android:layout_marginEnd="515dp"
        android:layout_marginBottom="248dp" />

    <Button
        android:id="@+id/ShowButton"
        android:layout_width="wrap_content"
        android:layout_height="76dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="464dp"
        android:layout_marginEnd="519dp"
        android:layout_marginBottom="91dp"
        android:text="Show" />

    <EditText
        android:id="@+id/UserText"
        android:layout_width="473dp"
        android:layout_height="156dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="26dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="525dp"
        android:layout_marginBottom="456dp"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/RedText"
        android:layout_width="25dp"
        android:layout_height="38dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="193dp"
        android:layout_marginEnd="909dp"
        android:layout_marginBottom="234dp"
        android:scrollbars="horizontal"
        android:text="R"
        android:textAppearance="@style/TextAppearance.AppCompat.Large" />

    <TextView
        android:id="@+id/GreenText"
        android:layout_width="24dp"
        android:layout_height="35dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="259dp"
        android:layout_marginEnd="911dp"
        android:layout_marginBottom="171dp"
        android:text="G"
        android:textAppearance="@style/TextAppearance.AppCompat.Large" />

    <TextView
        android:id="@+id/BlueText"
        android:layout_width="19dp"
        android:layout_height="43dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="314dp"
        android:layout_marginEnd="915dp"
        android:layout_marginBottom="107dp"
        android:text="B"
        android:textAppearance="@style/TextAppearance.AppCompat.Large" />

    <TextView
        android:id="@+id/Output"
        android:layout_width="519dp"
        android:layout_height="345dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center_vertical"
        android:layout_marginStart="505dp"
        android:layout_marginTop="145dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="141dp"
        android:text="Hello"
        android:textAlignment="center"
        android:textAppearance="@android:style/TextAppearance.Large"
        android:textSize="181sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/rNumber"
        android:layout_width="68dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="415dp"
        android:layout_marginTop="193dp"
        android:layout_marginEnd="541dp"
        android:layout_marginBottom="408dp" />

    <TextView
        android:id="@+id/gNumber"
        android:layout_width="68dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="414dp"
        android:layout_marginTop="248dp"
        android:layout_marginEnd="542dp"
        android:layout_marginBottom="353dp" />

    <TextView
        android:id="@+id/bNumber"
        android:layout_width="68dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="414dp"
        android:layout_marginTop="310dp"
        android:layout_marginEnd="542dp"
        android:layout_marginBottom="291dp" />

</RelativeLayout>